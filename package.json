{
  "name": "next-husky",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "next": "15.1.2",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
   "src/**/*.{ts,tsx}": [
    "npm run prettier:fix",
    "npm run lint"
   ]
  }
}
